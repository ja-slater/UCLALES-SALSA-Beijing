  sed_aero = .FALSE.          ! Calculate sedimentation of aerosol particles
  sed_cloud = .FALSE.          ! - '' - cloud droplets
  sed_precp = .FALSE.          ! precipitation
  sed_ice = .FALSE.            ! ice particles
  sed_snow = .FALSE.           ! snow flakes/precipitating ice
  itsflg = 0                  ! Flag for temperature type in input sounding
  ipsflg = 0                  ! - '' - for pressure
  lnudging = .FALSE.           ! Master swtich for nudging scheme
  lemission = .FALSE.          ! Master switch for aerosol emissions  
  iradtyp = 3                 ! Radiation/large scale forcing
